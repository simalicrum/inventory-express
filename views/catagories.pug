extends layout

block content
  ul(class="")
    each catagory in catagory_list
      li(class="m-8")
        span(class="my-4 text-md text-green-400 bg-green-100  border-green-200 py-2 px-4 rounded-md")
          a(href=catagory.url) #{catagory.name}
        span(class="my-4 ml-8 text-sm text-gray-400") #{catagory.description}
            